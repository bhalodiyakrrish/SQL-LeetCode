SELECT contest_id, ROUND(COUNT(DISTINCT(r.user_id))*100.0/(SELECT COUNT(*) FROM Users),2) [percentage]
FROM Users u
JOIN Register r
ON u.user_id = r.user_id
GROUP BY contest_id ORDER BY [percentage] DESC, contest_id;

Users =
| user_id | user_name |
| ------- | --------- |
| 6       | Alice     |
| 2       | Bob       |
| 7       | Alex      |
Register =
| contest_id | user_id |
| ---------- | ------- |
| 215        | 6       |
| 209        | 2       |
| 208        | 2       |
| 210        | 6       |
| 208        | 6       |
| 209        | 7       |
| 209        | 6       |
| 215        | 7       |
| 208        | 7       |
| 210        | 2       |
| 207        | 2       |
| 210        | 7       |
